---
import DefaultPosts from "./Posts/Default.astro";
import MediaPosts from "./Posts/Media.astro";
import VaultPosts from "./Posts/Vault.astro";

const { tag, year, posts } = Astro.props;
---

{
  (() => {
    switch (tag) {
      case "media":
        return <MediaPosts posts={posts} />;
      case "vault":
        return <VaultPosts tag={tag} year={year} posts={posts} />;
      default:
        return <DefaultPosts tag={tag} year={year} posts={posts} />;
    }
  })()
}