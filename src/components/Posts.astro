---
import MediaPosts from "./MediaPosts.astro";
import DefaultPosts from "./DefaultPosts.astro";
import VaultPosts from "./VaultPosts.astro";

const { tag, year, posts } = Astro.props;
---

{
  (() => {
    switch (tag) {
      case "media":
        return <MediaPosts posts={posts} />;
      case "vault":
        return <VaultPosts tag={tag} year={year} posts={posts} />;
      default:
        return <DefaultPosts tag={tag} year={year} posts={posts} />;
    }
  })()
}
