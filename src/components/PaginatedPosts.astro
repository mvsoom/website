---
import PaginatedDefaultPosts from "./PaginatedPosts/Default.astro";
import PaginatedMediaPosts from "./PaginatedPosts/Media.astro";

const { tag, years, target } = Astro.props;
---

{
  (() => {
    switch (tag) {
      case "media":
        return <PaginatedMediaPosts tag={tag} years={years} target={target} />;
      default:
        return <PaginatedDefaultPosts tag={tag} years={years} target={target} />;
    }
  })()
}